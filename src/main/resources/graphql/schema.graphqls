type Query{
    languages: [Language!]
    lexemes(filters: [LexemesFilter!]): [Lexeme!]
}

input LexemesFilter{
    languageCode: String
    token: String
}

type Language{
    name: String
    shortIsoCode: String
}

type Lexeme{
    headword: String
    language: Language
    languageCode: String
    senses: [Sense!]
}

type Sense{
    languageCode: String
    headword: String
    ordinal: Int
    value: String
}

